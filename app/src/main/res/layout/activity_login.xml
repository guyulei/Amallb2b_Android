<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/colorffffff"
    android:orientation="vertical"
    tools:context="com.amall360.amallb2b_android.ui.activity.LoginActivity">

    <include
        layout="@layout/public_layout_imagtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></include>

    <include
        android:id="@+id/username"
        layout="@layout/public_layout_texteditimge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"></include>

    <include
        android:id="@+id/password"
        layout="@layout/public_layout_texteditimge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></include>


</LinearLayout>
